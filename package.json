    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "papaparse": "^5.4.1",
    "openai": "^4.20.1",
    "langchain": "^0.0.200",
    "@langchain/openai": "^0.0.14",
    "@langchain/community": "^0.0.20",
    "chromadb": "^1.7.3",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
